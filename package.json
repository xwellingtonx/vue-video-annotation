{
  "name": "vue-video-annotation",
  "description": "Vue3 component that allows adding annotations to videos by free drawing or adding shapes like circles, squares, and arrows.",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --config vite.config.ts",
    "build:lib": "vite build --config vite-lib.config.ts",
    "serve": "vite preview"
  },
  "dependencies": {
    "fabric": "^5.2.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.36",
    "vue-fullscreen": "^3.1.1"
  },
  "devDependencies": {
    "@iconify/vue": "^3.2.1",
    "@types/fabric": "^4.5.7",
    "@types/node": "^17.0.35",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.36",
    "sass": "^1.52.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0",
    "vue-tsc": "^0.2.2"
  },
  "main": "./dist/vue-video-annotation.umd.js",
  "module": "./dist/vue-video-annotation.es.js",
  "files": [
    "dist/*"
  ],
  "author": "Wellington Pires",
  "repository": {
    "type": "git",
    "url": "https://github.com/xwellingtonx/vue-video-annotation.git"
  },
  "bugs": {
    "url": "https://github.com/xwellingtonx/vue-video-annotation/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/vue-video-annotation.es.js",
      "require": "./dist/vue-video-annotation.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/types/index.d.ts"
}
